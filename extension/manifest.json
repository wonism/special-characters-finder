{
  "manifest_version": 2,
  "name": "Special characters finder",
  "description": "Chrome extension for using special characters easily.",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}

